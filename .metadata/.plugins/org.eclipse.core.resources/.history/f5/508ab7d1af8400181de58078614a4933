package jse;

public class Hero {

	private String name;
	
	public void kill(Mortal m){
		m.die();
	}
	
	public static void main(String[] args){
		Hero h = new Hero();
		h.name = "gailun";
		ADHero ad = new ADHero();
		APHero ap = new APHero();
		ADAPHero adap = new ADAPHero();
		h.kill(ad);
		h.kill(ap);
		h.kill(adap);
	}
}
